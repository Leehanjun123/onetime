{
  "name": "onetime-backend",
  "version": "2.0.0",
  "description": "원데이 일자리 매칭 플랫폼 - 엔터프라이즈 마이크로서비스 버전",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "ts-node src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "echo 'Building application...' && npx prisma generate && echo 'Prisma client generated' && tsc && echo 'TypeScript compiled'",
    "postinstall": "npx prisma generate && echo 'Prisma client generated after install'",
    "db:migrate": "npx prisma migrate deploy",
    "db:push": "npx prisma db push",
    "db:reset": "npx prisma migrate reset",
    "db:studio": "npx prisma studio",
    "test": "jest",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@types/express-validator": "^2.20.33",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "redis": "^5.8.2",
    "uuid": "^13.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@jest/globals": "^30.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.1",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.3",
    "eslint": "^9.35.0",
    "jest": "^30.1.3",
    "jest-environment-node": "^30.1.2",
    "nodemon": "^3.1.10",
    "prisma": "^6.15.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "일데이",
    "job-matching",
    "daily-work",
    "api",
    "backend"
  ]
}
